{
    "name": "xenokore/logger",
    "version": "1.0.0",
    "type": "library",
    "description": "Simple wrapper for monolog",
    "keywords": ["logger"],
    "homepage": "https://xenokore.com",
    "license": "MIT",
    "authors": [{
        "name": "Yani B.",
        "email": "yani@xenokore.com",
        "homepage": "https://xenokore.com",
